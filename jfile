pipeline{
  agent any
  options{
    builddiscarder(logRotator(numtoKeepStr: '2', artifactnumtoKeepStr: '1'))
  }
  stages {
     stage('build') {
       steps{
         sh 'ant -f build.xml -v'
       }
     }
  }

  post {
    always{
      archiveArtifacts artifacts: 'dist/*.jar', fingerprint: true
    }
  }
}
